<div class="aurora-tab  {{=it.activeclass}} ">
    <div class="aurora-tab-focus-helper"></div>
    <div class="aurora-tab-content" aurora-slot="main">

        {{? it.useiconname }}
         <i aria-hidden="true" class="material-icons aurora-button-icon"><!---->{{=it.icon}}<!----></i>
        {{?}}
        {{? it.useiconurl }}
            <img src="{{=it.icon}}">
        {{?}}
        <div>{{=it.label}}</div>
    </div>
</div>
